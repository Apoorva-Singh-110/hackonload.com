<script>
    $(window).on('load', () => {
        $('.loader').delay(500).fadeOut('slow');
    });
    $(document).ready(() => {
        $(window).on('scroll', () => {
            if ($(window).scrollTop() < 100) {
                $('.header').removeClass('sticky_header');
            } else {
                $('.header').addClass('sticky_header');
            }
        });
    });
</script>
